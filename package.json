{
  "name": "near-educoin-nft-minting-contract-and-minting-page",
  "version": "0.1",
  "license": "(MIT AND Apache-2.0)",
  "scripts": {
    "deploy": "npm run build && cd contract && near dev-deploy out/series.wasm && export NFT_CONTRACT_ID=$(cat neardev/dev-account)",
    "build": "bash build.sh && npm run build-front",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "cd contract && cargo test",
    "test:integration": "npm run build:contract && cd integration-tests && npm test  -- -- \"./contract/target/wasm32-unknown-unknown/release/educoin_near_minting_contract.wasm\"",
    "deps-install": "npm install && cd ./integration-tests && npm install && cd ..",
    "dev": "cd frontend && next dev",
    "build-front": "cd frontend && next build",
    "start": "cd frontend && next start",
    "lint": " cd frontend && next lint"
  },
  "devDependencies": {
    "aos": "^2.3.4",
    "bootstrap": "^5.2.2",
    "near-api-js": "^0.45.1",
    "near-cli": "^3.3.0",
    "next": "^13.1.6",
    "nft.storage": "^7.0.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.6.0",
    "react-dom": "^18.2.0",
    "react-drag-drop-files": "^2.3.7",
    "react-headroom": "^3.2.0",
    "react-icons": "^4.4.0",
    "react-loader-spinner": "^5.1.5",
    "react-numeric-input": "^2.2.3",
    "react-select": "^5.6.1",
    "react-tag-input-component": "^2.0.2",
    "styled-components": "^5.3.5",
    "swiper": "^8.2.2"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.2",
    "@tanstack/react-query": "^4.24.4",
    "@types/aos": "^3.0.4",
    "@types/node": "17.0.38",
    "@types/react": "18.0.10",
    "@types/react-dom": "18.0.5",
    "@types/react-headroom": "^2.2.2",
    "@types/react-numeric-input": "^2.2.4",
    "@types/styled-components": "^5.1.25",
    "axios": "^1.3.2",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint": "8.16.0",
    "eslint-config-next": "12.1.6",
    "fuzzy-search": "^3.2.1",
    "mongodb": "^5.0.1",
    "mongoose": "^6.9.1",
    "react-content-loader": "^6.2.0",
    "react-redux": "^8.0.5",
    "react-toastify": "^9.1.1",
    "typescript": "4.7.2"
  }
}
